---
import Layout from './Layout.astro';

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <main class='max-w-7xl container mx-auto'>
    <article class='text-base prose prose-zinc p-5 max-w-7xl'>
      <slot />
    </article>
  </main>
</Layout>
